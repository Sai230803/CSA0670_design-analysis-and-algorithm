#include<stdio.h> 
int main() {
    int  n, k,i,fact=1,fact2=1,x,fact3=1,c;
    printf("\n Enter the value of n: ");     
    scanf("%d", &n);     
    printf("\n Enter the value of k: ");     
    scanf("%d", &k);
   for(i=1;i<=n;i++)
{
    fact=fact*i;
}
for(i=1;i<=k;i++)
{
    fact2=fact2*i;
}
if(n>=k)
{
  x=n-k;
  for(i=1;i<=x;i++)
  {
     fact3*=i; 
  }
   c=fact/(fact2*fact3);
  printf("binomial coefficient:%d",c);
}
  else
  printf("give n>k");
  return 0;
}
